class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int     size = prices.size();
        int     max;
        vector<int>     profit(size,0);
        for(int i = 0;i < size - 1;i++)
        {
            for(int j = i + 1;j < size;j++)
            {
                profit[j] = prices[j] - prices[i] ;
            }
            max = *max_element(profit.begin(),profit.end());
        }
        return max;
    }
};

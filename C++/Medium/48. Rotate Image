class Solution {
public:
    void rotate(vector<vector<int>>& matrix) {
        int n = matrix.size();
        int arr[n][n] = {0};
        for(int i = 0;i < n;i++)
            for(int j = 0;j < n;j++)
                arr[j][i] = matrix[i][j];
        for(int i = 0;i < n;i++)
            for(int j = n -1;j >= 0;j--)
                matrix[i][j] = arr[i][n-1-j];
    }
};
